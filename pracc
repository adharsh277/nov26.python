# aarguments


def my_function(fname):
    print(fname + "dsjfbf")


my_function("emil")
my_function("hgujhsg")
my_function("jbsauocx")
# Artibuitary arguments


def my_function(country="norway"):
    print("i am the king of "+country)
    my_function("sweden")
    my_function("switz")
    my_function("wellington")
    my_function("netherlads")

# python lambda


def x(a): return a+15


print(x(5))
# sub
def x(a): return a-7


print(x(7))
# multiply
def x(a): return a*3


print(x, (3, 4))

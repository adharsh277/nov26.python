# default parameter value


def my_function(country="norway"):
    print("iam  from" + country)


my_function(" sweden")
my_function(" netherland")
my_function(" canada")
my_function(" imdia")
# else in for loop
for x in range(6):
    print(x)
else:
    print("finally finished")


# Arguments


